{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby / Sanity boilerplate",
  "author": "Piotr Krasny",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:unit": "jest --passWithNoTests",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "clear_jest": "jest --clearCache",
    "storybook": "SET NODE_ENV=production & start-storybook -p 6006 --no-dll  -s public",
    "build-storybook": "NODE_ENV=production & build-storybook --no-dll -s public"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@parcel/namer-default": "^2.5.0",
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@testing-library/react": "^13.3.0",
    "algoliasearch": "^4.13.1",
    "babel-plugin-remove-graphql-queries": "^4.21.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "gatsby": "^4.18.0",
    "gatsby-plugin-algolia": "^0.26.0",
    "gatsby-plugin-google-gtag": "^4.18.0",
    "gatsby-plugin-image": "^2.18.0",
    "gatsby-plugin-manifest": "^4.18.0",
    "gatsby-plugin-offline": "^5.18.0",
    "gatsby-plugin-perf-budgets": "^0.0.18",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.18.0",
    "gatsby-plugin-sitemap": "^5.18.0",
    "gatsby-plugin-styled-components": "^5.18.0",
    "gatsby-plugin-utils": "^3.12.0",
    "gatsby-source-sanity": "^7.4.2",
    "gatsby-transformer-sharp": "^4.18.0",
    "gsap": "^3.10.4",
    "hamburger-react": "^2.5.0",
    "jest-environment-jsdom": "^28.1.3",
    "mapbox-gl": "^2.9.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icon": "^1.0.0",
    "react-instantsearch-dom": "^6.30.0",
    "react-is": "^18.2.0",
    "react-mapbox-gl": "^5.1.1",
    "react-share": "^4.4.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^28.1.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.21",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-preset-gatsby": "^2.20.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.5.1",
    "react-test-renderer": "^18.2.0",
    "storybook-addon-gatsby": "^0.0.5",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie 11",
    "not chrome < 51",
    "not safari < 10",
    "not android < 51"
  ]
}
