{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby / Sanity boilerplate",
  "author": "Piotr Krasny",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:unit": "jest --passWithNoTests",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "clear_jest": "jest --clearCache",
    "storybook": "SET NODE_ENV=production & start-storybook -p 6006 --no-dll  -s public",
    "build-storybook": "NODE_ENV=production & build-storybook --no-dll -s public"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "algoliasearch": "^4.14.2",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "esm": "^3.2.25",
    "gatsby": "^4.23.0",
    "gatsby-plugin-algolia": "^0.26.0",
    "gatsby-plugin-google-gtag": "^4.23.0",
    "gatsby-plugin-image": "^2.23.0",
    "gatsby-plugin-manifest": "^4.23.0",
    "gatsby-plugin-offline": "^5.23.0",
    "gatsby-plugin-perf-budgets": "^0.0.18",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^5.23.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.23.0",
    "gatsby-plugin-sitemap": "^5.23.0",
    "gatsby-plugin-styled-components": "^5.23.0",
    "gatsby-plugin-utils": "^3.17.0",
    "gatsby-source-sanity": "^7.4.2",
    "gatsby-transformer-sharp": "^4.23.0",
    "gsap": "^3.11.1",
    "hamburger-react": "^2.5.0",
    "jwt-decode": "^3.1.2",
    "mapbox-gl": "^2.10.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icon": "^1.0.0",
    "react-instantsearch-dom": "^6.33.0",
    "react-mapbox-gl": "^5.1.1",
    "react-share": "^4.4.0",
    "styled-components": "^5.3.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie 11",
    "not chrome < 51",
    "not safari < 10",
    "not android < 51"
  ],
  "devDependencies": {
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
