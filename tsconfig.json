{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "src/*": [
          "./src/*"
        ],
        "components/*": ["components/*"],
        "styles/*": ["styles/*"],
      },
      // "paths": {
      //   "components/*": ["components/*"],
      //   "styles/*": ["styles/*"],
      // },
      "target": "es5",
      "module": "es6",
      "allowJs": true,
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./dist/",
      "strictNullChecks": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    },
    "include": ["./src/", "custom.d.ts", "./jest-setup.ts"],
  }
  